{
  "name": "vpressify",
  "version": "1.0.7",
  "description": "Vite and React powered SSG",
  "keywords": [
    "vite",
    "ssg",
    "static site generator",
    "ssg-docs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-poc/vpressify.git"
  },
  "bugs": {
    "url": "https://github.com/v-poc/vpressify/issues"
  },
  "homepage": "https://github.com/v-poc/vpressify#readme",
  "author": "vpressify contributors",
  "engines": {
    "node": ">= 14.18.0",
    "pnpm": ">= 7"
  },
  "packageManager": "pnpm@7.28.0",
  "type": "module",
  "files": [
    "bin",
    "dist",
    "tailwind.config.cjs",
    "client.d.ts",
    "theme.d.ts",
    "shim.d.ts",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": "./dist/node/index.js",
    "./client": "./dist/client/index.js",
    "./theme": "./dist/theme/index.ts"
  },
  "bin": {
    "vpressify": "bin/vpressify.js"
  },
  "scripts": {
    "dev": "run-p -l dev:*",
    "dev:node": "tsc -w -p src/node",
    "dev:client": "tsc -w -p src/client",
    "dev:theme": "node scripts/copyTheme.js -w",
    "build": "rimraf dist && run-p -l build:*",
    "build:node": "tsc -p src/node",
    "build:client": "tsc -p src/client",
    "build:theme": "node scripts/copyTheme.js"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.3",
